<form action="/position-update" method="POST" style="width: 450px;">
    <h1 class="container">Sửa thông tin chức vụ</h1>
    <div class="form-group container">
      <input type="hidden" class="form-control" id="id" name="id" value="<%= positionCurrent._id  %>">
    </div>
    <div class="form-group container">
      <label for="chucvu">Chức vụ:</label>
      <input type="text" class="form-control" id="chucvu" name="chucvu" value=" <%= positionCurrent.name  %>" >
    </div>
    <div class="form-group container">
      <label for="luong">Lương:</label>
      <input type="text" class="form-control" id="luong" name="luong" value=" <%= positionCurrent.salary  %>" >
    </div>
    <div class="form-group container mt-2 mr-4">
      <button type="submit" class="btn btn-primary ">Sửa</button>
    </div>
  </form>
  
